<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>AddToBoardServlet</servlet-name>
        <servlet-class>Servlet.AddToBoardServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PaidServlet</servlet-name>
        <servlet-class>Servlet.PaidServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetCostServlet</servlet-name>
        <servlet-class>Servlet.GetCostServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CostServlet</servlet-name>
        <servlet-class>Servlet.CostServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddToBoardServlet</servlet-name>
        <url-pattern>/AddToBoard</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PaidServlet</servlet-name>
        <url-pattern>/Paid</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetCostServlet</servlet-name>
        <url-pattern>/GetCost</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CostServlet</servlet-name>
        <url-pattern>/Cost</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
